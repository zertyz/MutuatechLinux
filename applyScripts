  xxdiff files/home/common/bin/ /home/common/bin/
  xxdiff files/home/common/sbin/ /home/common/sbin/
  xxdiff files/home/common/docs/ /home/common/docs/
  xxdiff files/home/common/jwmrc /home/common/.jwmrc
  xxdiff files/home/common/xinitrc /home/common/.xinitrc
  xxdiff files/home/common/bash_logout /home/common/.bash_logout
